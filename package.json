{
  "name": "owphiyr-smart-home",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "serve": "npx vite --no-open --host=192.168.100.56 --port=8100",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "test:e2e": "cypress run",
    "test:unit": "vitest",
    "lint": "npx prettier --write . && npx eslint --fix --ext .js,.ts,.vue .",
    "start": "npx cap run android --target=Pixel_2_API_35 --live-reload --external --no-sync --port=8100 --host=192.168.100.56"
  },
  "dependencies": {
    "@capacitor-community/bluetooth-le": "^6.0.1",
    "@capacitor/android": "6.1.2",
    "@capacitor/app": "6.0.1",
    "@capacitor/core": "6.1.2",
    "@capacitor/device": "^6.0.1",
    "@capacitor/haptics": "6.0.1",
    "@capacitor/keyboard": "6.0.2",
    "@capacitor/status-bar": "6.0.1",
    "@ionic/storage": "^4.0.0",
    "@ionic/vue": "8.3.2",
    "@ionic/vue-router": "8.3.2",
    "axios": "^1.7.7",
    "ionicons": "^7.0.0",
    "vue": "3.5.11",
    "vue-router": "^4.2.0",
    "winston": "^3.15.0",
    "winston-syslog": "^2.7.1"
  },
  "devDependencies": {
    "@capacitor/cli": "6.1.2",
    "@vitejs/plugin-legacy": "^5.0.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.3.0",
    "concurrently": "^9.0.1",
    "cypress": "^13.5.0",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-vue": "^9.9.0",
    "jsdom": "^22.1.0",
    "prettier": "^3.3.3",
    "terser": "^5.4.0",
    "typescript": "^5.1.6",
    "vite": "5.4.8",
    "vitest": "^0.34.6",
    "vue-tsc": "^2.0.22"
  },
  "description": "An Ionic project"
